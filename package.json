{
  "name": "joseffie-gulp-pack",
  "description": "Starter template and gulp settings for web development",
  "author": "Ivan Gavrilich (joseffie)",
  "license": "GPL-3.0-only",
  "version": "3.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/joseffie/gulp-joseffie-settings.git"
  },
  "main": "gulpfile.babel.js",
  "type": "module",
  "scripts": {
    "dev": "gulp",
    "build": "gulp build --production",
    "zip": "gulp zip --production",
    "build:pug": "gulp pug --production",
    "build:scss": "gulp scss --production",
    "build:js": "gulp js --production",
    "build:images": "gulp images --production",
    "build:fonts": "gulp fonts",
    "build:sprites": "gulp sprites",
    "deploy": "npm run build && gh-pages -d dist",
    "publish": "gh-pages -d dist",
    "lint:scss": "npx stylelint \"src/**/*.scss\"",
    "lint:scss --fix": "npx stylelint \"src/**/*.scss\" --fix",
    "lint:js": "eslint --ext=mjs,js --debug src/**/*.js",
    "lint:js --fix": "eslint --ext=mjs,js --debug src/**/*.js --fix",
    "lint:pug": "pug-lint src/"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.10",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-function-sent": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.18.10",
    "@babel/register": "^7.18.9",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-imports": "^2.0.0",
    "bem-tools-core": "^0.0.3",
    "bem-tools-create": "^2.3.0",
    "browser-sync": "^2.27.10",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "circular-dependency-plugin": "^5.2.2",
    "del": "^7.0.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "emitty": "^1.4.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "gh-pages": "^4.0.0",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-data": "^1.3.1",
    "gulp-fonter": "^0.3.0",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^8.0.0",
    "gulp-newer": "^1.4.0",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-posthtml": "^3.0.5",
    "gulp-pug": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.1.0",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-ttf2woff2": "^4.0.1",
    "gulp-version-number": "^0.2.4",
    "gulp-webp": "^4.0.1",
    "gulp-webp-html-nosvg": "^1.1.1",
    "gulp-webpcss": "^1.1.1",
    "gulp-zip": "^5.1.0",
    "gulplog": "^2.0.0",
    "happypack": "^5.0.1",
    "posthtml-attrs-sorter": "^1.1.0",
    "prettier": "^2.7.1",
    "pug-lint": "^2.6.0",
    "sass": "^1.54.1",
    "stylelint": "^14.9.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "terser-webpack-plugin": "^5.3.3",
    "webp-converter": "^2.2.3",
    "webpack": "^5.74.0",
    "webpack-stream": "^7.0.0",
    "webpcss": "^1.3.4"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "lodash": "^4.17.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.pug": "pug-lint src/",
    "linters": {
      "*.{sass,scss}": [
        "npm run lint:styles --fix",
        "git add"
      ],
      "*.(js,jsx,mjs)": [
        "npm run lint:js --fix",
        "git add"
      ]
    }
  },
  "resolutions": {
    "gift": "^0.10.2"
  }
}
